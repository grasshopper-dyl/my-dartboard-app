generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

model Users {
  UserId    String   @id(map: "PK_Users") @db.VarChar(255)
  UserName  String   @unique(map: "Index_Username") @db.NVarChar(255)
  Email     String   @unique(map: "Index_Email") @db.VarChar(320)
  Password  String   @db.VarChar(Max)
  CreatedAt DateTime
}
